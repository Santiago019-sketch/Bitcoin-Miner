-- Initialize necessary services
local HttpService = game:GetService("HttpService")

-- Your RapidAPI key
local apiKey = "ea3f904005msh9dfe5b64e8638f2p1dd343jsn4d229b970349"

-- Define the URL for the API status endpoint
local url = "https://exercisedb.p.rapidapi.com/status"

-- Define the headers for the request
local headers = {
    ["x-rapidapi-host"] = "exercisedb.p.rapidapi.com",
    ["x-rapidapi-key"] = apiKey
}

-- Function to fetch API status
local function fetchApiStatus()
    local success, response = pcall(function()
        return HttpService:JSONDecode(HttpService:RequestAsync({
            Url = url,
            Method = "GET",
            Headers = headers
        }))
    end)

    if success then
        print("API Status:", response)
    else
        print("Failed to retrieve API status:", response)
    end
end

-- Call the function to fetch and print the API status
fetchApiStatus()
